{
  "name": "Django Codespace",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/workspace",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.10"
    }
  },
    "remoteEnv": {
    "PYTHONUNBUFFERED": "1"
  },

  "postCreateCommand": "pip install -r requirements.txt"

}
